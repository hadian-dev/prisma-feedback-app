    "react-dom": "17.0.2",
    "react-hook-form": "^7.23.0",
    "react-hot-toast": "^2.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.2",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.8",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prisma": "^3.8.0",
    "tailwindcss": "^3.0.13",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
